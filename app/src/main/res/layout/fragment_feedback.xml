<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:background="@drawable/image2"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="We need your Feedback!"
        android:textColor="@color/darkblue"
        android:textStyle="italic"
        android:id="@+id/textView15"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/darkblue"
        android:textStyle="italic"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Knowledge:"
        android:id="@+id/textView16"
        android:layout_marginTop="17dp"
        android:layout_alignTop="@+id/ratingBar"
        android:layout_alignLeft="@+id/textView17"
        android:layout_alignStart="@+id/textView17" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Communication Skills:"
        android:id="@+id/textView17"
        android:textColor="@color/darkblue"
        android:textStyle="italic"
        android:layout_marginTop="19dp"
        android:layout_below="@+id/ratingBar"
        android:layout_toStartOf="@+id/ratingBar2"
        android:layout_toLeftOf="@+id/ratingBar2" />

    <TextView
        android:layout_width="wrap_content"
        android:textColor="@color/darkblue"
        android:textStyle="italic"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Method of Teaching:"
        android:id="@+id/textView18"
        android:layout_below="@+id/ratingBar2"
        android:layout_alignLeft="@+id/textView17"
        android:layout_alignStart="@+id/textView17"
        android:layout_marginTop="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:textColor="@color/darkblue"
        android:textStyle="italic"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Quality of Assignments:"
        android:id="@+id/textView19"
        android:layout_marginTop="17dp"
        android:layout_below="@+id/ratingBar3"
        android:layout_alignLeft="@+id/textView18"
        android:layout_alignStart="@+id/textView18" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Approachability:"
        android:id="@+id/textView20"
        android:textColor="@color/darkblue"
        android:textStyle="italic"
        android:layout_marginTop="19dp"
        android:layout_below="@+id/ratingBar4"
        android:layout_alignLeft="@+id/textView19"
        android:layout_alignStart="@+id/textView19" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratingBar"
        android:layout_marginTop="42dp"
        android:layout_below="@+id/textView15"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratingBar2"
        android:layout_below="@+id/ratingBar"
        android:layout_alignLeft="@+id/ratingBar"
        android:layout_alignStart="@+id/ratingBar" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratingBar3"
        android:layout_below="@+id/ratingBar2"
        android:layout_alignLeft="@+id/ratingBar2"
        android:layout_alignStart="@+id/ratingBar2" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratingBar4"
        android:layout_below="@+id/ratingBar3"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratingBar5"
        android:layout_below="@+id/ratingBar4"
        android:layout_toRightOf="@+id/textView17"
        android:layout_toEndOf="@+id/textView17" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit"
        android:textColor="@color/darkblue"
        android:textStyle="italic"
        android:background="@color/colorPrimaryDark"
        android:id="@+id/button2"
        android:onClick="onStarClick"
        android:layout_below="@+id/ratingBar5"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="44dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Subject 1:"
        android:textColor="@color/darkblue"
        android:textStyle="italic"
        android:id="@+id/textView21"
        android:layout_above="@+id/textView16"
        android:layout_toStartOf="@+id/button2"
        android:layout_toLeftOf="@+id/button2" />
</RelativeLayout>